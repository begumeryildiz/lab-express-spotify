{{#if body.items}}
<div id="tracks-wrapper">
    <table id="tracks">
        <tr>
            <th>
                <h2>Title</h2>
            </th>
            <th>
                <h2 class="title-two">Listen</h2>
            </th>
        </tr>
        {{#each body.items}}
        <tr>
            <td>
                <p>{{this.name}}</p>
            </td>
            <td>
                <audio controls src="{{this.preview_url}}">
                    Your browser does not support the audio element.
                </audio>
            </td>
        </tr>

        {{/each}}
    </table>
</div>
{{/if}}